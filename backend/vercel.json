
{
  "version": 2,
  "builds": [
    { "src": "backend/nodejs/server.ts", "use": "@vercel/node" },
    { "src": "backend/golang/main.go", "use": "@vercel/go" },
    { "src": "frontend/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/toanime", "dest": "/backend/nodejs/server.ts" },
    { "src": "/(.*)", "dest": "/frontend/index.html" }
  ]
}
